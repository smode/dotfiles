# ~/.screenrc

escape          ^z^z
startup_message off
nethack         on
autodetach      on
defencoding     utf-8
encoding        utf-8 utf-8
cjkwidth       on
altscreen       on
hardstatus alwayslastline "%H %`%-w%{=b bw}%n %t%{-}%+w"

multiuser on 

defbce "on"
#term xterm-256color

screen -t main  0


